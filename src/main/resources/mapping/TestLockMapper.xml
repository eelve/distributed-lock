<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cachae.mapper.TestLockDAO">
	<select id="getVoById" resultType="cachae.entity.TestLockVo">
		SELECT * FROM SYNLOCK
		WHERE id = #{id}
	</select>

	<update id="updateVoById" parameterType="cachae.entity.TestLockVo">
		UPDATE
        SYNLOCK
		<set>
			id = #{id},
			name = #{name},
			countnum = #{countnum}
		</set>
		WHERE id = #{id}
	</update>


</mapper>